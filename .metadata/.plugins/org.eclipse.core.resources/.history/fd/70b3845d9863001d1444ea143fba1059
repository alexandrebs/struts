package beans.actions;

public class SaudarAction {


	private String bemVindoAtr;
	

	
	public String execute() {
		System.out.println("executando método struts2");
		this.bemVindoAtr = "Olá Struts2 com convenção";
		return "SUCCESS";
	}
	
	public String getBemVindoAtr() {
		return bemVindoAtr;
	}

	public void setBemVindoAtr(String bemVindoAtr) {
		this.bemVindoAtr = bemVindoAtr;
	}
	
	
	
}
